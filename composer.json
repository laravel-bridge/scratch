{
    "name": "laravel-bridge/scratch",
    "description": "For project from scratch",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "homepage": "https://mileschou.github.io/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/container": "^5.6 | ^6",
        "illuminate/support": "^5.6 | ^6"
    },
    "require-dev": {
        "ext-pdo": "*",
        "illuminate/database": "^5.6 | ^6",
        "illuminate/filesystem": "^5.6 | ^6",
        "illuminate/http": "^5.6 | ^6",
        "illuminate/pagination": "^5.6 | ^6",
        "illuminate/translation": "^5.6 | ^6",
        "illuminate/view": "^5.6 | ^6",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^6 | ^7 | ^8",
        "psy/psysh": "^0.9.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Scratch\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
